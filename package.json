  "scripts": {
    "prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js --env sentryRelease",
    "prod-local": "./node_modules/.bin/webpack --mode production --config webpack.prod-nosentry.js",
    "dev": "./node_modules/.bin/webpack --mode development --progress --color --stats-error-details --config webpack.dev.cjs --env sentryRelease",
    "dev:watch": "./node_modules/.bin/webpack --mode development --progress --color --config webpack.dev.js --env sentryRelease --watch",
    "deploy": "./buildRelease.sh",
    "lint": "eslint src --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules nyc jest --passWithNoTests",
    "testUTC": "TZ=utc nyc jest",
    "testDbg": "node --inspect-brk node_modules/.bin/jest",
    "testUTCDbg": "TZ=utc node --inspect-brk node_modules/.bin/jest",
    "start": "node dist/server/index.js",
    "build": "webpack --mode production --config webpack.prod_heroku.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfkeinberg/route-forecast.git"
  },
  "keywords": [
    "GPS",
    "weather",
    "bicycling",
    "randonneuring"
  ],
  "author": "Brian Feinberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bfkeinberg/route-forecast/issues"
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "homepage": "https://github.com/bfkeinberg/route-forecast#readme",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/jsx/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|htm)$": "<rootDir>/__mocks__/fileMock.js",
      "@google-cloud/datastore": "<rootDir>/__mocks__/@google-cloud/datastore.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "setupFiles": [
      "<rootDir>/setupFile.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!luxon)"
    ]
  }
}
